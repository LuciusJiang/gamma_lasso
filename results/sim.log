> eval <- function(y, f){
+ 	f <- as.matrix(f)
+ 	sst <- sum( (y-mean(y))^2 )
+ 	sse <- colSums( (y-f)^2 )
+ 	r2 <- 1-sse/sst
+ 	return(r2)
+ }
